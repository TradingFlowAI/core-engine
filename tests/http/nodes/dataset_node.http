### 数据集节点测试
@baseUrl = http://localhost:7000

### 获取所有节点类型信息
# @name get_node_types
GET {{baseUrl}}/nodes/types
Accept: application/json

### 执行Google Sheets数据集节点测试 - 读取模式
# @name execute_dataset_node_read
POST {{baseUrl}}/nodes/execute
Content-Type: application/json
Accept: application/json

{
  "flow_id": "example_flow",
  "component_id": 3,
  "cycle": 1,
  "node_id": "dataset_node_read",
  "node_type": "dataset_node",
  "input_edges": [],
  "output_edges": [
    {
      "source": "dataset_node_read",
      "source_handle": "data_output_handle",
      "target": "code_node_1",
      "target_handle": "input_data_0"
    }
  ],
  "config": {
    "node_class_type": "dataset_node",
    "spreadsheet_id": "1Q1J0-2s0SsY4z6uo9LXxTgYyacpeMrLlY7zbQnBYKgY",
    "worksheet_name": "Sheet1",
    "mode": "read",
    "range": "A1:Z1000",
    "header_row": true
  }
}


